{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/styles/*": [
        "./src/assets/styles/*"
      ],
      "@/components": [
        "./src/components/index.ts"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/content/*": [
        "./src/content/*"
      ],
      "@/components/ui/*": [
        "./src/components/ui/*"
      ],
      "@/components/base/*": [
        "./src/components/base/*"
      ],
      "@site-config": [
        "./src/site.config.ts"
      ],
      "@icon-config": [
        "./src/icon.config.ts"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    ".astro/types.d.ts",
    ".astro/content.d.ts",
    ".astro/icon.d.ts",
    "**/*"
  ],
  "exclude": [
    "./dist",
    "./node_modules",
  ],
}
